<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  <servlet-name>Add</servlet-name>
  <servlet-class>com.jsp.servlet.Addnum</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>Sub</servlet-name>
  <servlet-class>com.jsp.servlet.Subnum</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>Multi</servlet-name>
  <servlet-class>com.jsp.servlet.Multinum</servlet-class>
  </servlet>
  
  
  <servlet>
  <servlet-name>Div</servlet-name>
  <servlet-class>com.jsp.servlet.Divnum</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>Mod</servlet-name>
  <servlet-class>com.jsp.servlet.Modnum</servlet-class>
  </servlet>
  
   <servlet-mapping>
   <servlet-name>Add</servlet-name>
   <url-pattern>/add</url-pattern>
   </servlet-mapping>
   
   <servlet-mapping>
   <servlet-name>Sub</servlet-name>
   <url-pattern>/sub</url-pattern>
   </servlet-mapping>
   
   <servlet-mapping>
   <servlet-name>Multi</servlet-name>
   <url-pattern>/mul</url-pattern>
   </servlet-mapping>
   
   <servlet-mapping>
   <servlet-name>Div</servlet-name>
   <url-pattern>/div</url-pattern>
   </servlet-mapping>
   
   <servlet-mapping>
   <servlet-name>Mod</servlet-name>
   <url-pattern>/mod</url-pattern>   
   </servlet-mapping>
  
</web-app>
